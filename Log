				Label.BorderSizePixel = 0
				Label.Size = UDim2.new(0.899999976, 0,0, 20)
				Label.Font = Enum.Font.GothamBold
				Label.RichText = true
				Label.Text = Title
				Label.TextColor3 = Color3.fromRGB(255,255,255)
				Label.TextSize = 14
				Label.TextXAlignment = GetAlig(Side)

				local Index = {}

				function Index:Visible(v)
					Label.Visible = v
				end

				return Index
			end

